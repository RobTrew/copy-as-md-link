    // Left :: a -&gt; Either a b
    const Left = x =&gt; ({
        type: 'Either',
        Left: x
    });


    // Right :: b -&gt; Either a b
    const Right = x =&gt; ({
        type: 'Either',
        Right: x
    });


    // Tuple (,) :: a -&gt; b -&gt; (a, b)
    const Tuple = a =&gt;
