    // https://github.com/RobTrew/prelude-jxa

    // defaultOrFromFirst :: b -&gt; (a -&gt; b) -&gt; [a] -&gt; b
    const defaultOrFromFirst = v =&gt;
        // Equivalent of maybe | either for
        // the list monad. 
        f =&gt; xs =&gt; 0 &lt; xs.length ? (
            f(xs[0])
        ) : v;

    // MAIN ---
    return main();
