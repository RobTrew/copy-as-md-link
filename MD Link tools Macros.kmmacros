    // titlePosnSizeFromKindleIconLR :: Window -&gt; 
    // Either String [String, (Int, Int), (Int, Int)]
    const titlePosnSizeFromKindleIconLR = window =&gt; {
        const lists = window.lists;
        return bindLR(
            0 &lt; lists.length ? (
                Right(lists.at(0))
            ) : Left('No icon list found - perhaps not icon view ?')
