    // splitAt :: Int -&gt; [a] -&gt; ([a], [a])
    const splitAt = n =&gt;
        xs =&gt; [
            xs.slice(0, n),
            xs.slice(n)
        ];

    // until :: (a -&gt; Bool) -&gt; (a -&gt; a) -&gt; a -&gt; a
    const until = p =&gt;
        f =&gt; x =&gt; {
            let v = x;
            while (!p(v)) v = f(v);
            return v;
        };

