                )(['AXTitle', 'AXDocument'])
            return Boolean(maybeDocURL) ? (
                Right(`[${winTitle}](${maybeDocURL})`)
            ) : Left(
                `Window '${winTitle}' of:\n\n\t${bundleID}` + [
                    '\n\nmay not be a document window.',
                    `\nConsider adding a macro named '${bundleID}'`,
                    `to the KM Group '${kmGroupName}'.`,
