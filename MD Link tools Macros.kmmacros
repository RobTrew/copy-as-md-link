            alert('Copy as MD Link')
        )(
            s =&gt; (
                kme.setvariable('mdLink', {
                    to: s
                }),
                s
            )
        )(
            bindLR(
                clipTextLR()
            )(
                url =&gt; Right(`[${itemName}](${url})`)
            )
        );
    };

